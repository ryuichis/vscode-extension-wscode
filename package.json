{
  "name": "wscode",
  "displayName": "WSCode",
  "description": "Unofficial extension that enables you to chat with Cerebras Inference directly within VSCode",
  "version": "0.9.5",
  "publisher": "ryuichiban",
  "repository": "https://github.com/ryuichis/vscode-extension-wscode",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Cerebras Inference",
    "AI",
    "Machine Learning",
    "Chat"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "icon": "resources/wscode.png",
  "contributes": {
    "commands": [
      {
        "command": "wscode.ask",
        "title": "WSCode: Ask Cerebras Inference"
      },
      {
        "command": "wscode.setupApiKey",
        "title": "WSCode: Setup API Key for Cerebras Inference"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "WSCode",
      "properties": {
        "wscode.apiKey": {
          "type": "string",
          "default": "",
          "description": "API Key for Cerebras Inference"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "wscodeView",
          "title": "WSCode",
          "icon": "resources/wscode.svg"
        }
      ]
    },
    "views": {
      "wscodeView": [
        {
          "type": "webview",
          "id": "cerebrasInferenceView",
          "name": "Cerebras Inference"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "@cerebras/cerebras_cloud_sdk": "^1.15.0",
    "marked": "^15.0.4"
  }
}
